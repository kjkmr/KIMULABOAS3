package {	import flash.display.Sprite;	import flash.display.Shape;	import jp.kimulabo.utils.ResizeManager;		public class BasicUsage extends Sprite {						/*--------------------------------------------------		* コンストラクタ		--------------------------------------------------*/		public function BasicUsage() {						stage.align = "LT";			stage.scaleMode = "noScale";						ResizeManager.init( stage );						var blue:Shape = new Shape();			blue.graphics.beginFill( 0x336699 );			blue.graphics.drawCircle( 0, 0, 30 );			addChild( blue );			ResizeManager.add( blue, { x:0.5, y:0.5 } );		}	}}