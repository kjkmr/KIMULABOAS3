package {	import flash.display.Sprite;	import flash.display.Shape;	import flash.events.Event;	import jp.kimulabo.display.bezier.*;		import caurina.transitions.Tweener;		public class BasicUsage01 extends Sprite {				private var _container:Sprite = new Sprite();				private var _from:BezierPath = new BezierPath(<path fill="#FFFFFF" stroke="#000000" d="M0,0v100c100,50,0,100,0,100v100h100"/>);		private var _to:BezierPath = new BezierPath(<path fill="#FFFFFF" stroke="#000000" d="M0,0l50,50c100,150,0,100,0,100c100,150,0,100,0,100c100,150,0,100,0,100"/>);				/*--------------------------------------------------		* BezierPathの基本的な使い方（１）		* 		* 		--------------------------------------------------*/		public function BasicUsage01() {						stage.align = "LT";			stage.scaleMode = "noScale";						addChild( _container );						var m:PathMotion = new PathMotion( _container.graphics, _from, _to );						Tweener.addTween( m, { position:1, time:2, delay:1, transition:"easeOutElastic" } );					}			}}